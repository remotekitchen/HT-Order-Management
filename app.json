{
  "expo": {
    "name": "Order-Management",
    "slug": "order-management",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/main_icon_300x300.png",
    "scheme": "order-management",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.chatchefs.ordermanagement",
      "supportsTablet": true,
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "UIBackgroundModes": [
          "audio",
          "fetch",
          "remote-notification",
          "background-processing"
        ]
      }
    },
    "android": {
      "package": "com.chatchefs.ordermanagement",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/main_icon_300x300.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "INTERNET",
        "NOTIFICATIONS",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "FOREGROUND_SERVICE",
        "WAKE_LOCK",
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS",
        "android.permission.POST_NOTIFICATIONS"
      ],
      "notificationSounds": {
        "order_sound": "./assets/sound/order_sound.wav"
      },
      "notificationChannels": [
        {
          "id": "high_importance_channel",
          "name": "High Importance Notifications",
          "description": "Channel for important order notifications",
          "importance": "high",
          "sound": "order_sound",
          "vibration": true,
          "lights": true
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "sounds": ["./assets/sound/order_sound.wav"]
        }
      ],
      [
        "expo-audio",
        {
          "microphonePermission": "Allow Order-Management to access your microphone for order notification sounds."
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-background-fetch",
        {
          "startOnBoot": true,
          "stopOnTerminate": false
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "extra": {
      "eas": {
        "projectId": "2cb7747a-5834-4310-8ac7-08bcac33471e"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/2cb7747a-5834-4310-8ac7-08bcac33471e"
    }
  }
}
